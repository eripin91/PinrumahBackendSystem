                @{int i = 0;}
                @foreach (var item in Model.Listings)
                {
                    string carouselid = "carouselExampleIndicators" + @i;
                    string targetCarouselId = "#" + carouselid;
                    @i++;

                    var agent = await UserManager.FindByIdAsync(item.CreatedBy);
                    string avatar = string.Empty;
                    if (agent != null && !string.IsNullOrEmpty(agent.avatar_link))
                    {
                        avatar = agent.avatar_link;
                    }
                    else avatar = "/theme/dist/img/avatar-blank.png";
