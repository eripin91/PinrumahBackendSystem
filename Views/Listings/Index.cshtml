                        <div class="branding branding--extra-large " style="background-color: aliceblue">
                            <picture>
                                <source srcset="/theme/dist/img/logopinrumah.png" type="image/webp">
                                <img class="branding__image" alt="Pinrumah" src="/theme/dist/img/logopinrumah.png">
                            </picture>
                            <div class="agent agent--with-photo" aria-label="@item.AgentName">
                                <p class="agent__name" aria-hidden="true" style="color: black">@item.AgentName</p>
                                <span class="agent__photo" aria-hidden="true">
                                    <picture>
                                        <img alt="@item.AgentName" class="agent__photo-image" src="@avatar">
                                    </picture>
                                </span>
                            </div>
                        </div>


                        <div class="box-body box-profile">
                            <!--CAROUSEL CODE GOES HERE-->

                            <div id=@carouselid class="carousel slide" data-ride="carousel">
                                <ol class="carousel-indicators">
                                    <li data-target=@targetCarouselId data-slide-to="0" class="active"></li>
                                    @if (!string.IsNullOrEmpty(item.ImagePath2))
                                    {
                                        <li data-target=@targetCarouselId data-slide-to="1"></li>
                                    }
                                    @if (!string.IsNullOrEmpty(item.ImagePath3))
                                    {
                                        <li data-target=@targetCarouselId data-slide-to="2"></li>
                                    }
                                    @if (!string.IsNullOrEmpty(item.ImagePath4))
                                    {
                                        <li data-target=@targetCarouselId data-slide-to="3"></li>
                                    }
                                    @if (!string.IsNullOrEmpty(item.ImagePath5))
                                    {
                                        <li data-target=@targetCarouselId data-slide-to="4"></li>
                                    }
                                </ol>
                                <a asp-action="Details" asp-route-id="@item.Id">
                                    <div class="carousel-inner _carousel-container">
                                        @{
                                            if (!string.IsNullOrEmpty(item.ImagePath1))
                                            {
                                                <div class="carousel-item active">
                                                    <img class="d-block w-100 img-carousel _img-carousel-list" src="@Url.Content(item.ImagePath1)" alt="@Html.DisplayFor(model => item.ImagePath1)">
                                                </div>
                                            }
                                            if (!string.IsNullOrEmpty(item.ImagePath2))
                                            {
                                                <div class="carousel-item">
                                                    <img class="d-block w-100 img-carousel _img-carousel-list" src="@Url.Content(item.ImagePath2)" alt="@Html.DisplayFor(model => item.ImagePath2)">
                                                </div>
                                            }
                                            if (!string.IsNullOrEmpty(item.ImagePath3))
                                            {
                                                <div class="carousel-item">
                                                    <img class="d-block w-100 img-carousel _img-carousel-list" src="@Url.Content(item.ImagePath3)" alt="@Html.DisplayFor(model => item.ImagePath3)">
                                                </div>
                                            }
                                            if (!string.IsNullOrEmpty(item.ImagePath4))
                                            {
                                                <div class="carousel-item">
                                                    <img class="d-block w-100 img-carousel _img-carousel-list" src="@Url.Content(item.ImagePath4)" alt="@Html.DisplayFor(model => item.ImagePath4)">
                                                </div>
                                            }
                                            if (!string.IsNullOrEmpty(item.ImagePath5))
                                            {
                                                <div class="carousel-item">
                                                    <img class="d-block w-100 img-carousel _img-carousel-list" src="@Url.Content(item.ImagePath5)" alt="@Html.DisplayFor(model => item.ImagePath5)">
                                                </div>
                                            }

                                        }

                                    </div>
                                </a>
                                <a class="carousel-control-prev" href=@targetCarouselId role="button" data-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a class="carousel-control-next" href=@targetCarouselId role="button" data-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </a>
                            </div>

                            <!--Carousel code end -->
                            <div class="_list-description-box">
                                <h3 class="profile-username"><span>PR</span>@Html.DisplayFor(model => item.PropertyId)</h3>

                                <p class="text-muted">@Html.DisplayFor(model => item.Title)</p>
                                <p class="text-muted">
                                    @Html.DisplayFor(model => item.Address)<br />
                                    <span class="text-green text-lg">@Html.DisplayFor(model => item.Price)</span>
                                </p>
                                <i class="fa fa-bed light"></i> @Html.DisplayFor(model => item.NoOfBed) &nbsp;&nbsp;
                                <i class="fa fa-bath"></i>@Html.DisplayFor(model => item.NoOfBath)&nbsp;&nbsp;
                                <i class="fa fa-car"></i>@Html.DisplayFor(model => item.NoOfGarage) &nbsp;&nbsp; | &nbsp;&nbsp;
                                @Html.DisplayFor(model => item.PropertyType)
                            </div>
                        </div>
                        <!-- /.box-body -->
